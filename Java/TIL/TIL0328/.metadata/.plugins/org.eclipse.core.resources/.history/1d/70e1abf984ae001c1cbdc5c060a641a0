
public class TypeConversion {
	public static void main(String[] args) {
		byte b = 127;
		int i = 100;
		
		System.out.println(b+i);
		System.out.println(10/4);
		System.out.println(b+i);
		System.out.println(b+i);
		System.out.println(b+i);
		System.out.println(b+i);
		System.out.println(b+i);
		System.out.println(b+i);
	}
}
