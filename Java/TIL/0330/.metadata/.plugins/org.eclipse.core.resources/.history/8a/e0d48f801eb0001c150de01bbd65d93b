
public class report1_1 {

	public static void main(String[] args) {
		// 1에서 100 까지의 정수 중에서 2,3,5,7의 배수를 제외한 수를 
		// 한 행에 10개씩 출력하는 프로그램 작성하기.
		int counters = 0;
		
		for (int i = 1; i <= 100; i++) {
			if (i%2==0 || i%3==0 || i%5==0 || i%7==0) {}
			else {
			System.out.print(i+" ");
			counters++;
			if (counters%10 == 0)
				System.out.println();
}
}
}
}
