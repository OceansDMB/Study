public class Problem_8{
	public static void main(String[] args) {
			int x,y,z;
			x=900;
			y=900;
			z=900;
			
			System.out.println("[4-2]: " +x+ "-"+y+"-"+z);
		//	[4-3]
			int x1=300,y1=400,z1=500;
			System.out.println("[4-3]: " +x1+ "-"+y1+"-"+z1);
			
		// [4-4] : 같은 값으로 초기화 
			String srt1,str2,str3;
			
			System.out.println(str3);
	}
}